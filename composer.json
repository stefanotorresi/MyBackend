{
    "name": "stefanotorresi/my-backend",
    "description": "A Zend Framework 2 module that glues a bunch of packages to lay down the base for an admin backend UI.",
    "type": "library",
    "keywords": [
        "zf2",
        "zend",
        "module",
        "user",
        "admin",
        "backend"
    ],
    "homepage": "https://github.com/stefanotorresi/MyBackend",
    "authors": [
        {
            "name": "Stefano Torresi",
            "email": "webdev@stefanotorresi.it",
            "homepage": "http://stefanotorresi.it"
        }
    ],
    "require": {
        "php": ">5.4.1",
        "doctrine/doctrine-module": "~0.8",
        "doctrine/doctrine-orm-module": "~0.8",
        "hounddog/doctrine-data-fixture-module": "0.*",
        "mwillbanks/zfc-twitter-bootstrap": "dev-master",
        "rwoverdijk/assetmanager": "1.*",
        "stefanotorresi/my-base": "~1.1,>=1.1.2",
        "zf-commons/zfc-rbac": "~2.1",
        "zf-commons/zfc-user": "~1.0",
        "zf-commons/zfc-user-doctrine-orm": "~1.0",
        "ocramius/lazy-property": "1.*"
    },
    "require-dev": {
        "zendframework/zendframework": "~2.3"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev",
            "dev-0.x": "0.x-dev"
        }
    },
    "autoload": {
        "psr-0": {
            "MyBackend": "src/",
            "MyBackend\\Test\\": "tests/"
        },
        "classmap": [
            "./"
        ]
    },
    "license": "MIT"
}
